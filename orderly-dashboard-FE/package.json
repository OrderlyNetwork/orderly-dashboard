{
  "name": "orderly-dashboard-fe",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "generate:css": "unocss './app/**/*.{js,ts,jsx,tsx}' -o ./app/styles/uno.css",
    "build": "run-s \"build:*\"",
    "build:css": "npm run generate:css",
    "build:remix": "remix vite:build",
    "dev": "run-p \"dev:*\"",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "node ./server.js",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "format": "prettier --write . && yarn eslint --fix .",
    "start": "cross-env NODE_ENV=production node ./server.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/server": "^11.11.0",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.16",
    "@mui/x-date-pickers": "^7.3.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.0.2",
    "@remix-run/express": "^2.9.0",
    "@remix-run/node": "^2.9.0",
    "@remix-run/react": "^2.9.0",
    "@tanstack/react-table": "^8.16.0",
    "@tarnadas/fixed-number": "^0.1.1",
    "compression": "^1.7.4",
    "dayjs": "^1.11.11",
    "express": "^4.19.2",
    "isbot": "^5.1.4",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "swr": "^2.2.5",
    "ts-pattern": "^5.1.1",
    "use-debounce": "^10.0.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.9.0",
    "@types/compression": "^1.7.5",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.9",
    "@types/react": "^18.2.79",
    "@types/react-dom": "^18.2.25",
    "@typescript-eslint/eslint-plugin": "^7.7.1",
    "@typescript-eslint/parser": "^7.7.1",
    "@unocss/cli": "^0.59.4",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "typescript": "5.4.5",
    "unocss": "^0.59.4",
    "vite": "^5.2.10",
    "vite-tsconfig-paths": "^4.3.2"
  }
}
